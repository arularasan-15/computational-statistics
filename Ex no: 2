import plotly.express as px
# Load the Iris dataset
df = px.data.iris()
# 1. Print the first 10 instances
print("1. First 10 instances:")
print(df.head(10))
# 2. Print the Number of Rows and Columns
print("\n2. Number of Rows and Columns:")
print(df.shape)
# 3. Print the Column names [Attribute Names]
print("\n3. Column names [Attribute Names]:")
print(df.columns.tolist())
# 4. Mean of all the Numerical Attributes
print("\n4. Mean of all the Numerical Attributes:")
print(df.mean(numeric_only=True))
